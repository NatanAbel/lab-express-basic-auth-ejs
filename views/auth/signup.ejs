<h1>Signup page</h1>
<form method="POST" action="/auth/signup">
    <label >
        Username:
        <input type="text" name="username" <% if (typeof errorMessag !== undefined) { %>
            value="<%= body.username %> "
        <% } %>/>
    </label>
    <label >
        Password:
        <input type="password" name="password" required/>
    </label>
    <button type="submit">Sign Up</button>
</form>